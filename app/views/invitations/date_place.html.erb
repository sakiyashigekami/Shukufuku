    <%= f.label "電話番号" %>
    <%= f.text_field :phonenumber2 %>

    <%= f.label "会場URL" %>
    <%= f.text_field :place_url2 %>

    <%= f.label "備考" %>
    <%= f.text_field :note2 %>

    <%= f.submit "保存" %>
